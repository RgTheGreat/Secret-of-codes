//functions
		package main

import "fmt"

func add(a int, b int) int {
	return a + b

}

func main() {
	fmt.Println(add(42, 13))
}

//named results-go
package main

import "fmt"

func split(sum int) (x, y int) {
	x = sum * 4 / 9
	y = sum - x
	return
}

func main() {
	fmt.Println(split(17))
}


//variables
package main

import "fmt"

var c, python, java bool

func main() {
	var i int
	fmt.Println(i, c, python, java)
}
