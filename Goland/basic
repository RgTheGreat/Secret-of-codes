var i int

fmt.println("i, c, python, java")

}


//function

package main

import "fmt"

func add(a int, b int) int {
 return a + b
}

func main() {
fmt.println(add(13, 45))
}


//named results-go
package main

import "fmt"

func split(sum int) (a, y int) {
	x = sum * 4 / 9
	y = sum - x
	return
}

func main() {
	fmt.Println(split(17))
}
