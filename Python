#This is only python not html
print("Hello World!")

#variables
x = 6
y = john
print(x)
print(y)

#numbers int,float,complex
x = 1
y = 2.5
z = 35e6
print(type(x))
print(type(y))
print(type(z))

#if else
a = 400
b = 225

if b > a:
print("b is greater than a")
else:
print("b is not greater than a")

#bools
x = "RTG"
y = 0
print(bool(x))
print(bool(y))

#lists
1-list = ["Python", "JS", "HTML",] 
print(1-list)
